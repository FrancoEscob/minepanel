{
  "name": "@minepanel/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run --passWithNoTests",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:push": "prisma db push --schema prisma/schema.prisma",
    "seed:owner": "tsx scripts/seed-owner.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@minepanel/contracts": "workspace:*",
    "@minepanel/domain": "workspace:*",
    "@minepanel/runtime": "workspace:*",
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.1",
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "fastify": "^5.6.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "prisma": "^6.16.2",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
